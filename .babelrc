{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/core"
        }
      }
    ]
  ],

  "plugins": [
    [
      "@emotion",
      {
        "autoLabel": "always",
        "labelFormat": "[local]",
        "cssPropOptimization": true
      }
    ]
  ]
}
